                      data: data.ys,\r\n                        pointRadius: 0,\r\n                        lineTension: 0,\r\n                        backgroundColor: \"rgba(220,53,70, 0.2)\",\r\n                        borderColor: \"rgba(220,53,70, 1)\",\r\n                        borderWidth: 1,\r\n                        responsive: true,\r\n                        mainAspectRatio: false,\r\n                        display: false,\r\n                    },\r\n                    {\r\n                        label: \"Daily Recovered COVID19 Cases : \" + state,\r\n                        data: data.rys,\r\n                        pointRadius: 0,\r\n                        lineTension: 0,\r\n                        backgroundColor: \"rgba(145,218,67, 0.2)\",\r\n                        borderColor: \"rgba(145,218,67, 1)\",\r\n                        borderWidth: 1,\r\n                        responsive: true,\r\n                        mainAspectRatio: false,\r\n                        type: \"line\",\r\n                    },\r\n                ],\r\n            },\r\n            options: {\r\n                scales: {\r\n                    xAxes: [\r\n                        {\r\n                            ticks: {\r\n                                display: false, //this will remove only the label\r\n                            },\r\n                            gridLines: {\r\n                                color: \"rgba(0, 0, 0, 0)\",\r\n                            },\r\n                        },\r\n                    ],\r\n                    yAxes: [\r\n                        {\r\n                            ticks: {\r\n                                display: false,\r\n                            },\r\n                            gridLines: {\r\n                                color: \"rbga(0,0,0,0)\",\r\n                            },\r\n                        },\r\n                    ],\r\n                },\r\n            },\r\n        });\r\n    }